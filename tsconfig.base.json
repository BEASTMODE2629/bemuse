{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "lib": ["es5", "es6", "dom"],
    "noUnusedLocals": true,
    "pretty": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "es6",
    "module": "commonjs",
    "jsx": "react"
  }
}
